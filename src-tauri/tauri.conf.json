{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ChmlFrpLauncher",
  "version": "0.5.7",
  "identifier": "net.chmlfrp.launcher",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "ChmlFrp Launcher",
        "width": 1030,
        "height": 660,
        "resizable": true,
        "fullscreen": false,
        "transparent": true,
        "decorations": true,
        "titleBarStyle": "Overlay",
        "trafficLightPosition": {
          "x": 12,
          "y": 20
        }
      }
    ],
    "security": {
      "csp": "default-src 'self' asset: tauri: http://localhost:5173; script-src 'self' 'unsafe-eval' http://localhost:5173; style-src 'self' 'unsafe-inline' http://localhost:5173; img-src 'self' asset: tauri: data: blob: app: file:; media-src 'self' asset: tauri: data: blob: app: file:; font-src 'self' asset: tauri: data:; connect-src 'self' https://cf-v2.uapis.cn https://cf-v1.uapis.cn https://chmlfrp.net https://*.chmlfrp.net https://github.com http://localhost:5173 ws://localhost:5173;"
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": "all",
    "windows": {
      "wix": {
        "language": "zh-CN"
      }
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDUyRERERkFFQjA5QjY1MDAKUldRQVpadXdydC9kVWtNdEpGaHVmK3puWUJpZUpvNnJ5ejdVTk1PdWZwSDArWVJsWEsydEJObTgK",
      "endpoints": [
        "https://cf-v2.uapis.cn/launcher/update",
        "https://github.com/TechCat-Team/ChmlFrpLauncher/releases/latest/download/latest.json"
      ]
    },
    "deep-link": {
      "desktop": {
        "schemes": ["chmlfrp"]
      }
    }
  }
}
